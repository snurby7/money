{
  "version": 1,
  "cli": {
    "analytics": false,
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "thunder",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    },
    "@nrwl/react": {
      "application": {
        "style": "@emotion/styled",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "@emotion/styled"
      },
      "library": {
        "style": "@emotion/styled",
        "linter": "eslint"
      }
    }
  },
  "projects": {
    "lightning": {
      "root": "apps/lightning",
      "sourceRoot": "apps/lightning/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/lightning",
            "index": "apps/lightning/src/index.html",
            "main": "apps/lightning/src/main.tsx",
            "polyfills": "apps/lightning/src/polyfills.ts",
            "tsConfig": "apps/lightning/tsconfig.app.json",
            "assets": [
              "apps/lightning/src/favicon.ico",
              "apps/lightning/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/lightning/src/environments/environment.ts",
                  "with": "apps/lightning/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "lightning:build",
            "proxyConfig": "apps/lightning/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "lightning:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/lightning/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/lightning"
          ],
          "options": {
            "jestConfig": "apps/lightning/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "lightning-e2e": {
      "root": "apps/lightning-e2e",
      "sourceRoot": "apps/lightning-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/lightning-e2e/cypress.json",
            "tsConfig": "apps/lightning-e2e/tsconfig.e2e.json",
            "devServerTarget": "lightning:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "lightning:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/lightning-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps\\api\\src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": [
              "apps/api/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/api/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/api"
          ],
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "contracts": {
      "root": "libs/contracts",
      "sourceRoot": "libs/contracts/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/contracts/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/contracts"
          ],
          "options": {
            "jestConfig": "libs/contracts/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "thunder": {
      "projectType": "application",
      "root": "apps/thunder",
      "sourceRoot": "apps/thunder/src",
      "prefix": "sample-playground",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/thunder",
            "index": "apps/thunder/src/index.html",
            "main": "apps/thunder/src/main.ts",
            "polyfills": "apps/thunder/src/polyfills.ts",
            "tsConfig": "apps/thunder/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/thunder/src/favicon.ico",
              "apps/thunder/src/assets"
            ],
            "styles": [
              "apps/thunder/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/thunder/src/environments/environment.ts",
                  "with": "apps/thunder/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "thunder:build",
            "proxyConfig": "apps/thunder/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "thunder:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "thunder:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/thunder/src/**/*.ts",
              "apps/thunder/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/thunder"
          ],
          "options": {
            "jestConfig": "apps/thunder/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "thunder-e2e": {
      "root": "apps/thunder-e2e",
      "sourceRoot": "apps/thunder-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/thunder-e2e/cypress.json",
            "tsConfig": "apps/thunder-e2e/tsconfig.e2e.json",
            "devServerTarget": "thunder:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "thunder:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/thunder-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    }
  }
}
